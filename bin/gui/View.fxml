<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <ComboBox fx:id="comboBoxPerson" layoutX="25.0" layoutY="30.0" onAction="#onComboxPersonAction" prefWidth="150.0" promptText="Select a Person..." />
      <Button fx:id="btAll" layoutX="222.0" layoutY="30.0" mnemonicParsing="false" onAction="#OnBtAllAction" text="All" />
   </children>
</AnchorPane>
